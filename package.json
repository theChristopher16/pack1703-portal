  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "test": "react-scripts test --env=jsdom --watchAll=false",
    "test:watch": "react-scripts test --env=jsdom",
    "test:coverage": "react-scripts test --env=jsdom --coverage --watchAll=false",
    "test:unit": "react-scripts test --env=jsdom --testPathPattern=src/__tests__/unit --watchAll=false",
    "test:integration": "react-scripts test --env=jsdom --testPathPattern=src/__tests__/integration --watchAll=false",
    "test:e2e": "react-scripts test --env=jsdom --testPathPattern=src/__tests__/e2e --watchAll=false",
    "test:services": "react-scripts test --env=jsdom --testPathPattern=src/__tests__/unit/services --watchAll=false",
    "test:components": "react-scripts test --env=jsdom --testPathPattern=src/__tests__/unit/components --watchAll=false",
    "test:auth": "react-scripts test --env=jsdom --testPathPattern=auth --watchAll=false",
    "test:user-management": "react-scripts test --env=jsdom --testPathPattern=user-management --watchAll=false",
    "test:ai": "react-scripts test --env=jsdom --testPathPattern=aiService --watchAll=false",
    "test:runner": "./scripts/test-runner.sh",
    "test:runner:unit": "./scripts/test-runner.sh unit",
    "test:runner:integration": "./scripts/test-runner.sh integration",
    "test:runner:e2e": "./scripts/test-runner.sh e2e",
    "test:runner:auth": "./scripts/test-runner.sh auth",
    "test:runner:user-management": "./scripts/test-runner.sh user-management",
    "test:runner:ai": "./scripts/test-runner.sh ai",
    "test:runner:coverage": "./scripts/test-runner.sh -c",
    "eject": "react-scripts eject",
    "deploy": "npm run build && firebase deploy --only hosting",
    "deploy:all": "npm run build && firebase deploy",
    "deploy:functions": "firebase deploy --only functions",
    "deploy:firestore": "firebase deploy --only firestore",
    "predeploy": "npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
